  "description": "nebula-graph-studio",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-nebula-graph-studio",
    "docker-start": "egg-scripts start --title=egg-server-nebula-graph-studio",
    "stop": "egg-scripts stop --title=egg-server-nebula-graph-studio",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.server.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint -c tslint.json 'app/**/*.{ts,tsx}' && stylelint **/*.{html,less}",
    "lint-fix": "tslint -c tslint.json 'app/**/*.{ts,tsx}' --fix && stylelint **/*.{html,less} --fix",
    "clean": "ets clean",
    "build": "webpack --config config/webpack.prod.ts"
  },
